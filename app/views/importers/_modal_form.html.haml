.modal{:style=>"display:none",:"aria-hidden"=>"true",:id=>dom_id(temp)}
  .modal-header
    %button.close{:type=>"button",:"data-dismiss"=>"modal",:"aria-hidden"=>true}
      ×
    %h3
      ="修改导入数据"
  .modal-body
    = simple_form_for(temp,:remote=>true,:url=>main_app.update_importer_temp_path(:temp_id=>temp.id),:wrapper => :bootstrap,:html => { :class => 'form-horizontal' }) do |f|
      .form-inputs
        = f.input :data,as: :text, input_html: { rows: 10 }
      .form-actions
        = f.button :submit#,:"data-dismiss"=>"modal",:"aria-hidden"=>true
